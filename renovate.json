{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "commitMessageSuffix": "in {{{replace 'src\\/(.*)\\/' '$1' packageFileDir}}}",
  "additionalBranchPrefix": "{{packageFileDir}}/",
  "branchConcurrentLimit": 10,
  "prConcurrentLimit": 10,
  "prHourlyLimit": 10
}
